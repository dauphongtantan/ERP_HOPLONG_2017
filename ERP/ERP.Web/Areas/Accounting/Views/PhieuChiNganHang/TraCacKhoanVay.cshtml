                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <input ng-model="info.dien_giai_noi_dung_thanh_toan" type="text" class="form-control" placeholder="Tạm Ứng Nhân Viên" id="diengiainoidung" ng-change="checklydo()">
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <div class="col-md-4">
                                                <div>Đối tượng</div>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" ng-model="info.ma_doi_tuong" ng-change="onDoiTuongFind()" ng-focus="showtable_ma_doi_tuong = true">
                                                    <div class="input-group-btn">
                                                        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" ng-click="showtable_ma_doi_tuong = !showtable_ma_doi_tuong">
                                                            <span class="caret"></span>
                                                        </button>
                                                        <div class="dropdown-menu dropdown-menu-center borderRadius0 padding0" style="display: block" ng-show="showtable_ma_doi_tuong">
                                                            <div class="table-responsive" style="max-height:400px; max-width:500px;">
                                                                <table class="table table-striped table-hover" st-table="listDoiTuongCollection" st-safe-src="listDoiTuong">
                                                                    <thead class="bg-blue text-white">
                                                                        <tr>
                                                                            <th>Mã đối tượng</th>
                                                                            <th>Tên đối tượng</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody ng-repeat="doi_tuong in listDoiTuongCollection">
                                                                        <tr style="cursor:pointer" ng-click="selectDoiTuong(doi_tuong)">
                                                                            <td>{{doi_tuong.MA_DOI_TUONG}}</td>
                                                                            <td>{{doi_tuong.TEN_DOI_TUONG}}</td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control" ng-model="info.ten_doi_tuong">
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <div>Địa chỉ</div>
                                    </div>
                                    <div class="col-md-10">
                                        <input type="text" class="form-control" ng-model="info.dia_chi">
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <div class="col-md-4">
                                                <div>TK nhận</div>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" ng-model="info.ncc_so_tai_khoan" ng-value="info.ncc_so_tai_khoan">
                                                    <div class="input-group-btn">
                                                        <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" ng-click="getNccTaiKhoanNganHang()">
                                                            <span class="caret"></span>
                                                        </button>
                                                        <div class="dropdown-menu dropdown-menu-center borderRadius0 padding0">
                                                            <div class="table-responsive">
                                                                <table class="table table-striped table-hover" st-table="listNccTknhCollection" st-safe-src="listNccTknh">
                                                                    <thead class="bg-blue text-white">
                                                                        <tr>
                                                                            <th>Số tài khoản</th>
                                                                            <th>Tên ngân hàng</th>
                                                                            <th>Chi nhánh</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody ng-repeat="ncc_tknh in listNccTknhCollection">
                                                                        <tr style="cursor:pointer" ng-click="selectNccTaiKhoanNganHang(ncc_tknh)">
                                                                            <td>{{ncc_tknh.SO_TAI_KHOAN}}</td>
                                                                            <td>{{ncc_tknh.TEN_NGAN_HANG}}</td>
                                                                            <td>{{ncc_tknh.CHI_NHANH}}</td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <input type="text" class="form-control" ng-model="info.ncc_ten_ngan_hang">
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <div class="col-md-4">
                                                <div>Nhân viên</div>
                                            </div>
                                            <div class="col-md-8">
                                                <div class="input-group">
                                                    <input type="text" class="form-control" ng-model="info.nhan_vien_thu" ng-change="onNhanVienFind()" ng-focus="showtable_ho_va_ten = true">
                                                    <div class="input-group-btn">
                                                        <button type="button" class="btn btn-default dropdown-toggle" ng-click="showtable_ho_va_ten = !showtable_ho_va_ten" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                            <span class="caret"></span>
                                                        </button>
                                                        <div class="dropdown-menu dropdown-menu-center borderRadius0 padding0" style="display: block" ng-show="showtable_ho_va_ten">
                                                            <div class="table-responsive" style="max-height: 280px">
                                                                <table class="table table-striped table-hover" st-table="staffsCollection" st-safe-src="arrayNVFinded">
                                                                    <thead class="bg-blue text-white">
                                                                        <tr>
                                                                            <th>Mã nhân viên</th>
                                                                            <th>Tên nhân viên</th>
                                                                            <th>Phòng ban</th>
                                                                            <th>Điện thoại di động</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        <tr ng-repeat="staff in staffsCollection" style="cursor:pointer" ng-click="selectNhanVienThu(staff)">
                                                                            <td>{{staff.USERNAME}}</td>
                                                                            <td>{{staff.HO_VA_TEN}}</td>
                                                                            <td>{{staff.TEN_PHONG_BAN}}</td>
                                                                            <td>{{staff.SDT}}</td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-2">
                                        <div>Tham chiếu</div>
                                    </div>

                                    <div class="col-md-6">
                                        <input type="hidden" value="@Session["USERNAME"]" id="username" />
                                        <input type="hidden" value="@Session["MA_CONG_TY"]" id="macongty" />
                                        <div class="form-control bootstrap-tagsinput" style="border: 1px solid #00BFFF !important;"><span class="tag label label-info" ng-repeat="item in ThamChieu.ListSelect">{{item.SO_CHUNG_TU}}<span data-role="remove" ng-click="RemoveThamChieu($index)"></span></span> <input style="border: none !important" type="text" placeholder=""></div>
                                    </div>
                                    <div class="col-lg-4">
                                        <a data-toggle="modal" data-target="#modal_theme_primary"><i class="fa fa-search" aria-hidden="true"></i></a>
                                    </div>
                                </div>
                            </div>
                        </fieldset>
                    </form>
                </div>
                <div class="col-md-4">
                    <form class="form-kt">
                        <fieldset>
                            <legend class="bold">Chứng từ</legend>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-5">
                                        <div>Ngày hạch toán</div>
                                    </div>
                                    <div class="col-md-7">
                                        <input type="text" id="ngay_hach_toan" class="form-control datetimepickerKT" ng-model="info.ngay_hach_toan">
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-5">
                                        <div>Ngày chứng từ</div>
                                    </div>
                                    <div class="col-md-7">
                                        <input type="text" id="ngay_chung_tu" class="form-control datetimepickerKT" ng-model="info.ngay_chung_tu">
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-5">
                                        <div>Số chứng từ</div>
                                    </div>
                                    <div class="col-md-7">
                                        <input type="text" class="form-control" ng-model="info.so_chung_tu">
                                    </div>
                                </div>
                            </div>
                            <br>
                            <br>
                            <br>
                            <br>
                            <br>
                            <br>
                            <br>
                        </fieldset>
                    </form>
                </div>
            </div>
            <br>
            <div class="row">
                <div class="col-md-7 col-md-offset-5">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="text-right">
                                <form class="form-inline">
                                    <div class="form-group">
                                        <label class="marginR15">Loại tiền</label>
                                        <select name="" class="form-control" ng-model="listHachToan[itemHachToanSelect].LOAI_TIEN">
                                            <option value="USD">USD</option>
                                            <option value="VND">VND</option>
                                            <option value="EURO">EURO</option>
                                        </select>
                                    </div>
                                </form>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="text-right">
                                <form class="form-inline" name="form">
                                    <div class="form-group" ng-show="listHachToan[itemHachToanSelect].LOAI_TIEN != 'VND'">
                                        <label class="marginR15">Tỷ giá</label>
                                        <input name="tyGia" format='number' float required type="text" class="form-control" ng-model="listHachToan[itemHachToanSelect].TY_GIA">
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <br>
            <div class="tab-content">
                <div class="table-responsive">
                    <table class="table table-striped table-hover table-bordered" st-table="listHachToanCollection" st-safe-src="listHachToan">
                        <thead class="bg-blue text-white">
                            <tr>
                                <th>Diễn giải</th>
                                <th>TK nợ</th>
                                <th>TK có</th>
                                <th>Số tiền</th>
                                <th ng-show="listHachToan[itemHachToanSelect].LOAI_TIEN != 'VND'">Quy đổi</th>
                                <th>Đơn vị</th>
                                <th>Mã đối tượng</th>
                                <th>Tên đối tượng</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td><a href="" ng-click="addHachToan()">Bấm vào đây để thêm mới</a></td>
                                <td>&nbsp;</td>
                                <td>&nbsp;</td>
                                <td>&nbsp;</td>
                                <td ng-show="listHachToan[itemHachToanSelect].LOAI_TIEN != 'VND'">&nbsp;</td>
                                <td>&nbsp;</td>
                                <td>&nbsp;</td>
                                <td>&nbsp;</td>
                                <td>&nbsp;</td>
                            </tr>
                            <tr ng-repeat="itemHachToan in listHachToanCollection" ng-click="onSelectItemHachToan($index)">
                                <td><input type="text" class="form-control" style="max-width: 170px;" ng-model="listHachToan[$index].DIEN_GIAI" ng-disabled="itemHachToanSelect != $index" id="diengiai"></td>
                                <td>
                                    <div class="input-group maxWidth150">
                                        <input type="text" class="form-control" ng-model="listHachToan[$index].TK_NO" ng-disabled="itemHachToanSelect != $index">
                                        <div class="input-group-btn">
                                            <button type="button" class="btn btn-default dropdown-toggle height_hachtoan" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" ng-disabled="itemHachToanSelect != $index">
                                                <span class="caret"></span>
                                            </button>
                                            <div class="dropdown-menu dropdown-menu-center borderRadius0 padding0">
                                                <div class="table-responsive" style="max-height:200px">
                                                    <table class="table table-striped table-hover " st-pagination-scroll st-table="tknoCollections" st-safe-src="listDmTaiKhoanHachToan">
                                                        <thead class="bg-blue text-white">
                                                            <tr>
                                                                <th>Số tài khoản</th>
                                                                <th>Tên Tài khoản</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="tk_no in tknoCollections | filter:listHachToan[$index].TK_NO" ng-click="selectTkNo(tk_no)" style="cursor:pointer">
                                                                <td>{{tk_no.SO_TK}}</td>
                                                                <td>{{tk_no.TEN_TK}}</td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="input-group maxWidth150">
                                        <input type="text" class="form-control" ng-model="listHachToan[$index].TK_CO" ng-disabled="itemHachToanSelect != $index">
                                        <div class="input-group-btn">
                                            <button type="button" class="btn btn-default dropdown-toggle height_hachtoan" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" ng-disabled="itemHachToanSelect != $index">
                                                <span class="caret"></span>
                                            </button>
                                            <div class="dropdown-menu dropdown-menu-center borderRadius0 padding0">
                                                <div class="table-responsive" style="max-height:200px">
                                                    <table class="table table-striped table-hover " st-pagination-scroll st-table="tkcoCollections" st-safe-src="listDmTaiKhoanHachToan">
                                                        <thead class="bg-blue text-white">
                                                            <tr>
                                                                <th>Số tài khoản</th>
                                                                <th>Tên Tài khoản</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="tk_co in tkcoCollections | filter:listHachToan[$index].TK_CO" ng-click="selectTkCo(tk_co)" style="cursor:pointer">
                                                                <td>{{tk_co.SO_TK}}</td>
                                                                <td>{{tk_co.TEN_TK}}</td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </td>
                                <td><input type="text" format='number' class="form-control maxWidth120" ng-model="itemHachToan.SO_TIEN" ng-disabled="itemHachToanSelect != $index"> </td>
                                <td ng-show="listHachToan[itemHachToanSelect].LOAI_TIEN != 'VND'">{{itemHachToan.SO_TIEN * itemHachToan.TY_GIA | currency: ''}}</td>
                                <td>{{itemHachToan.LOAI_TIEN}}</td>
                                <td> {{info.ma_doi_tuong}}</td>
                                <td>{{info.ten_doi_tuong}}</td>
                                <td class="text-center"><button ng-click="RemoveRow($index)" style="background: none !important; border: none !important;"><span class="glyphicon glyphicon-remove" style="color:red;"></span></button></td>
                            </tr>
                            <tr ng-repeat="item in arrayBlank">
                                <td>&nbsp;</td>
                                <td>&nbsp;</td>
                                <td>&nbsp;</td>
                                <td>&nbsp;</td>
                                <td ng-show="listHachToan[itemHachToanSelect].LOAI_TIEN != 'VND'">&nbsp;</td>
                                <td>&nbsp;</td>
                                <td>&nbsp;</td>
                                <td>&nbsp;</td>
                                <td>&nbsp;</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <br>
            <div class="text-center">
                <button class="btn btn-primary" ng-click="save()"><span class="glyphicon glyphicon-floppy-disk"></span> Lưu</button>
                <button class="btn btn-danger" ng-click="cancel()"><span class="glyphicon glyphicon-ban-circle"></span> Bỏ qua</button>
            </div>
        </div>
    </div>

    <div id="modal_theme_primary" class="modal fade" style="display: none;" data-backdrop="static">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header bg-primary-300">
                    <h5 class="modal-title" style="font-weight:700">Chọn chứng từ tham chiếu</h5>
                </div>
                <div class="modal-body">
                    <form class="form-horizontal">
                        <div class="form-group">
                            <label class="control-label col-lg-2">Tìm theo</label>
                            <div class="col-lg-3">
                                <select name="select" class="form-control" ng-change="ChangeLoaiChungTu()" ng-model="LoaiChungTu">
                                    <option value="1">Loại chứng từ</option>
                                    <option value="2">Đối tượng</option>
                                    <option value="3">Số hóa đơn/chứng từ</option>
                                </select>
                                <label id="basic-error" class="validation-error-label" ng-if="Validate.LoaiChungTu == false" for="basic">Trường này không được để trống</label>
                            </div>
                            <label class="control-label col-lg-2">Giá trị</label>
                            <div class="col-lg-5" style="position:relative;z-index:100">
                                <div class="has-feedback" id="Input_DataGiaTriChungTu" style="display:none" ng-click="ShowDataGiaTriChungTu()">
                                    <input type="text" ng-model="GiaTriChungTu.Search" class="form-control showtableselect" ng-change="DoiTuongFind()">
                                    <div class="form-control-feedback">
                                        <i class="icon-arrow-down5"></i>
                                    </div>
                                </div>
                                <select name="select" class="form-control" id="Select_DataGiaTriChungTu" ng-model="GiaTriLoaiChungTu">
                                    <option ng-repeat="item in GiaTriThamChieu" value="{{item.value}}">{{item.show}}</option>
                                </select>

                                <table class="table-hover tableselect" id="DataGiaTriChungTu" style="display:none">
                                    <thead>
                                        <tr>
                                            <th style="width:35%">Mã đối tượng</th>
                                            <th style="width:35%">Tên đối tượng</th>
                                            <th style="width:30%">Địa chỉ</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="item in GiaTriThamChieu | filter:GiaTriChungTu.Search" ng-click="SelectDataGiaTriChungTu(item)">
                                            <td>{{item.madoituong}}</td>
                                            <td>{{item.tendoituong}}</td>
                                            <td>{{item.diachi}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                                <div class="has-feedback" id="Input_MaChungTu" style="display:none">
                                    <input type="text" ng-model="MaChungTu.Search" class="form-control showtableselect">
                                </div>
                            </div>
                            <label id="basic-error" class="validation-error-label" ng-if="Validate.GiaTriChungTu == false" for="basic">Trường này không được để trống</label>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-lg-2">Từ ngày</label>
                            <div class="col-lg-3">
                                <div class="form-group has-feedback">
                                    <input type="text" datepicker ng-model="ThamChieu.From" id="ThamChieuFrom" class="form-control datetimepickerKT">
                                    <div class="form-control-feedback">
                                        <i class="icon-calendar"></i>
                                    </div>
                                    <label id="basic-error" class="validation-error-label" ng-if="Validate.FromDateThamChieu == false" for="basic">Trường này không được để trống</label>
                                </div>
                            </div>
                            <label class="control-label col-lg-2">Đến ngày</label>
                            <div class="col-lg-3">
                                <div class="form-group has-feedback">
                                    <input type="text" datepicker ng-model="ThamChieu.To" id="ThamChieuTo" class="form-control datetimepickerKT">
                                    <div class="form-control-feedback">
                                        <i class="icon-calendar"></i>
                                    </div>
                                    <label id="basic-error" class="validation-error-label" ng-if="Validate.ToDateThamChieu == false" for="basic">Trường này không được để trống</label>
                                    <label id="basic-error" class="validation-error-label" ng-if="Validate.ToDateThamChieuLess == false" for="basic">Ngày kết thúc phải lớn hơn ngày bắt đầu</label>
                                </div>
                            </div>
                            <div class="col-lg-2 text-right">
                                <button class="btn bg-primary-300" type="button" ng-disabled="CheckSearchThamChieu()" ng-click="SearchThamChieu()">Lấy dữ liệu</button>
                            </div>
                        </div>
                        <div class="col-md-12 content-group" style="">
                            <div class="table-responsive">
                                <table class="table table-bordered table-striped table-hover ">
                                    <thead>
                                        <tr class="bg-primary-300">
                                            <th style="width:10px;"></th>
                                            <th>Ngày hạch toán</th>
                                            <th>Ngày chứng từ</th>
                                            <th>Số chứng từ</th>
                                            <th>Diễn giải</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-cloak dir-paginate="item in ThamChieu.ListResult | itemsPerPage: numPerPage"
                                            pagination-id="ThamChieu" current-page="currentPage">
                                            <td><div class="checker border-primary-600 text-primary-800" ng-click="SelectThamChieu(item,$index)"><span ng-class="item.Action?'uncheck':''"><input type="checkbox" class="control-primary" uncheck="checked"></span></div></td>
                                            <td>
                                                {{item.NGAY_HACH_TOAN}}
                                            </td>
                                            <td>{{item.NGAY_CHUNG_TU}}</td>
                                            <td>{{item.SO_CHUNG_TU}}</td>
                                            <td>{{item.DIEN_GIAI}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                                <dir-pagination-controls pagination-id="ThamChieu"
                                                         class="pull-right"></dir-pagination-controls>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="modal-footer text-center">
                    <button type="button" class="btn btn-primary" ng-click="SetThamChieu()"><i class="icon-checkbox-checked"></i> Đồng ý</button>
                    <button type="button" class="btn bg-orange-400" data-dismiss="modal"><i class="icon-blocked"></i> Bỏ qua</button>
                </div>
            </div>
        </div>
    </div>
</div>
<style>
    .bg-blue {
        background-color: #5BC0DE !important;
    }
</style>