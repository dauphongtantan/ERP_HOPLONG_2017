<style>
    .form-control {
        border-style: none;
    }
</style>

<div ng-controller="nhacungcapCtrl">
    <div class="content" style="min-height:0px;padding: 0px !important;">
        <input type="hidden" id="nv" name="nv" value="@Session["USERNAME"]" />
        <input type="hidden" id="maphongban" name="maphongban" value="@Session["MA_PHONG_BAN"]" />
        <input type="hidden" id="macongty" name="macongty" value="@Session["MA_CONG_TY"]" />
        <input type="hidden" id="isadmin" name="isadmin" value="@Session["IS_AMIN"]" />
    </div>
    <div class="panel-heading">
        <marquee width="80%">Bạn đang xem danh mục Nhà cung cấp.</marquee>
    </div>
    <!-- Modal Add New nha cung cap -->
    <div class="modal fade" id="myModal" role="dialog" tabindex="-1" style="overflow-x: hidden;
    overflow-y: auto;">
        <div class="modal-dialog modal-lg" style="width: 90%">
            <div class="modal-content" style="height: 100%">
                <div class="mhead">
                    <h2>THÊM MỚI NHÀ CUNG CẤP</h2>
                </div>
                <div class="modal-body">
                    <form class="form-horizontal" action="/NhaCungCap/Index" method="post">
                        <p style="text-shadow: 1px 1px 5px #91DDFD; font-style:italic; font-size: 20px; color: #64B5F6; margin-left:10px; margin-top:5px;margin-bottom:5px; font-weight: bold">THÔNG TIN CHUNG</p>
                        <div class="line-bottom"></div>
                        <table border='1' style="width:100%" ng-controller="imgCtrl" id="themmoikhach">
                            <tr>
                                <th class="text_edit">Mã cung cấp</th>
                                <td colspan="7"><input type="text" class="form-control" id="addtencongty" placeholder="Mã nhà cung cấp" ng-model="arraythongtin.ma_nha_cung_cap"></td>

                                <td colspan='2' rowspan='4'>
                                    <p style="text-shadow: 1px 1px 5px #91DDFD; font-style:italic; font-size: 20px; color: #64B5F6; margin-left:10px; margin-top:5px; font-weight: bold">Ảnh đại diện</p>
                                    <img id="blah" src="~/Content/self-writing/Images/default_avatar_male.jpg" />
                                    @Html.AntiForgeryToken()
                                    <input type='file' name="files" id="imgInp" style="width: 100%;display: inline-block" />
                                </td>
                            </tr>
                            <tr>
                                <th class="text_edit">Tên nhà cung cấp</th>
                                <td colspan="7">
                                    <input type="text" class="form-control" id="addtencongty" placeholder="Tên nhà cung cấp" ng-model="arraythongtin.ten_nha_cung_cap">
                                </td>
                            </tr>
                            <tr>
                                <th class="text_edit">Văn phòng giao dịch</th>
                                <td colspan="7">
                                    <input type="text" placeholder="Văn phòng giao dịch" class="form-control" id="addvpgiaodich" ng-model="arraythongtin.van_phong_giao_dich">
                                </td>
                            </tr>
                            <tr>
                                <th class="text_edit">Địa chỉ xuất hóa đơn</th>
                                <td colspan="7">
                                    <input type="text" class="form-control" placeholder="Địa chỉ xuất hóa đơn" id="adddiachixuathoadon" ng-model="arraythongtin.dia_chi_xuat_hoa_don">
                                </td>
                            </tr>
                            <tr>
                                <th class="text_edit" style="width:15%">Phân loại nhà cung cấp</th>
                                <td colspan="1" style="width:15%">
                                    <select ng-model="arraythongtin.ma_loai_ncc" class="form-control col-sm-3 input-md" style="width: 100% !important">
                                        <option ng-repeat="phanloai in list_phanloai" value="{{phanloai.MA_LOAI_NCC}}">{{phanloai.TEN_LOAI_NCC}}</option>
                                    </select>
                                </td>
                                <th class="text_edit" style="width:10%">Loại hàng cung cấp</th>
                                <td colspan="5" style="width:65%">
                                    <div class="form-control bootstrap-tagsinput" style="border: none;">
                                        <span class="tag label label-info" ng-repeat="select in ListSelect">
                                            {{select.MA_NHOM_HANG_CHI_TIET}}
                                            <span data-role="remove" ng-click="RemoveList($index)"></span>
                                        </span>
                                        <input style="border: none !important" type="text" placeholder="Nhập vào ô tìm kiếm để chọn">
                                    </div>

                                </td>
                                <th><a data-toggle="modal" data-target="#modal_theme_primary"><i class="btn fa fa-search" aria-hidden="true"></i></a></th>
                            </tr>
                            <tr>
                                @*<td><label class="control-label">Loại hàng cung cấp</label></td>
                                            <td colspan="4" ng-repeat="vthh in list_nhomvthh">
                                        <input type='checkbox' value="{{vthh.MA_NHOM_HANG_CHI_TIET}}" check-list='checked_fruits' /><span>{{vthh.CHUNG_LOAI_HANG}}</span>
                                    </td>*@
                            </tr>
                            <tr>
                                <th class="text_edit">Mã số thuế</th>
                                <td colspan="1" style="width:15%">
                                    <input type="text" class="form-control" placeholder="Nhập mã số thuế" id="addmasothue" ng-model="arraythongtin.ma_so_thue">
                                </td>
                                <th class="text_edit">Website</th>
                                <td colspan="1" style="width:15%">
                                    <input type="text" class="form-control" placeholder="Nhập website" id="addwebsite" ng-model="arraythongtin.website">
                                </td>
                                <th class="text_edit">Số điện thoại</th>
                                <td colspan="1" style="width:15%">
                                    <input type="tel" class="form-control" placeholder="Nhập số điện thoại" id="addsomayban" ng-model="arraythongtin.sdt">
                                </td>
                                <th class="text_edit">Số Fax</th>
                                <td colspan="2" style="width:15%">
                                    <input type="text" class="form-control" placeholder="Nhập số Fax" id="addfax" ng-model="arraythongtin.fax">
                                </td>
                            </tr>
                            <tr>
                                <th class="text_edit">Điều khoản thanh toán</th>
                                <td colspan="1" style="width:15%">
                                    <select class="form-control" ng-model="arraythongtin.dieu_khoan_thanh_toan" id="adddieukhoanthanhtoan" data-ng-options="item as item for item in dieukhoantt"></select>
                                </td>
                                <th class="text_edit">Số ngày được nợ</th>
                                <td colspan="1" style="width:15%">
                                    <input type="number" class="form-control" placeholder="Số ngày được nợ" id="addsongayduocno" ng-model="arraythongtin.so_ngay_duoc_no">
                                </td>
                                <th class="text_edit">Số nợ tối đa</th>
                                <td colspan="1" style="width:15%">
                                    <input type="number" class="form-control" placeholder="Số nợ tối đa" id="addsonotoida" ng-model="arraythongtin.so_no_toi_da">
                                </td>
                                <th class="text_edit">Email</th>
                                <td colspan="2" style="width:15%">
                                    <input type="text" class="form-control" placeholder="Nhập Email" id="addemail" ng-model="arraythongtin.email">
                                </td>
                            </tr>

                            <tr>
                                <th class="text_edit">Ghi chú</th>
                                <td colspan='3'>
                                    <textarea name="addghichu" placeholder="Ghi chú" class="form-control input-md" ng-model="arraythongtin.ghi_chu"></textarea>
                                </td>
                                <th class="text_edit">Đánh giá</th>
                                <td colspan='5'>
                                    <textarea name="danh_gia" class="form-control input-md" ng-model="arraythongtin.danh_gia"></textarea>
                                </td>
                            </tr>
                        </table>
                        <table>
                            <p style="text-shadow: 1px 1px 5px #91DDFD; font-style:italic; font-size: 20px; color: #64B5F6; margin-left:10px; margin-top:15px;margin-bottom:5px; font-weight: bold">TÀI KHOẢN NHÀ CUNG CẤP</p>
                            <div class="line-bottom"></div>
                            <thead>
                                <tr>
                                    <th>Số tài khoản</th>
                                    <th colspan="2">Tên tài khoản</th>
                                    <th colspan="2">Tên ngân hàng</th>
                                    <th colspan="3">Chi nhánh</th>
                                    <th>Tỉnh thành phố</th>
                                    <th colspan="2">Loại tài khoản</th>
                                </tr>
                            </thead>
                            <tr ng-repeat="(index,item) in arraytaikhoan">
                                <td><input type="text" class="form-control" ng-model="arraytaikhoan[$index].so_tai_khoan"></td>
                                <td colspan="2"><input type="text" class="form-control" ng-model="arraytaikhoan[$index].ten_tai_khoan"></td>
                                <td colspan="2"><input type="text" class="form-control" ng-model="arraytaikhoan[$index].ten_ngan_hang"></td>
                                <td colspan="3"><input type="text" class="form-control" ng-model="arraytaikhoan[$index].chi_nhanh"></td>
                                <td><input type="text" class="form-control" ng-model="arraytaikhoan[$index].tinh_tp"></td>
                                <td colspan="2">
                                    <select ng-model="arraytaikhoan[$index].loai_tai_khoan" class="form-control input-md">
                                        <option ng-repeat="loaitaikhoan in list_loaitaikhoan" value="{{loaitaikhoan.MA_LOAI}}">{{loaitaikhoan.TEN_LOAI}}</option>
                                    </select>
                                </td>
                            </tr>
                        </table>
                        <button type="button" class="fa fa-plus button_addrow" ng-click="AddNew()"><span style="font-size: 15px; font-weight: bold; padding-left:5px; padding-top:5px;">Thêm dòng</span></button>

                        <table>
                            <p style="text-shadow: 1px 1px 5px #91DDFD; font-style:italic; font-size: 20px; color: #64B5F6; margin-left:10px; margin-top:15px;margin-bottom:5px; font-weight: bold">THÔNG TIN LIÊN HỆ</p>
                            <div class="line-bottom"></div>
                            <thead>
                                <tr>
                                    <th class="text-center">Người liên hệ</th>
                                    <th class="text-center">Chức vụ</th>
                                    <th class="text-center">Phòng ban</th>
                                    <th class="text-center">Ngày sinh</th>
                                    <th class="text-center">Giới tính</th>
                                    <th class="text-center">SĐT</th>
                                    <th class="text-center">Email</th>
                                    <th class="text-center">Skype</th>
                                    <th class="text-center">Facebook</th>
                                    <th class="text-center">Pur phụ trách</th>
                                </tr>
                            </thead>
                            <tr ng-repeat="(index,item) in arraylienhe">
                                <td><input type="text" style="height: 100%;padding-left: 5px;width:100%" class="form-control" ng-model="arraylienhe[$index].nguoi_lien_he"></td>
                                <td><input type="text" style="height: 100%;padding-left: 5px;width:100%" class="form-control" ng-model="arraylienhe[$index].chuc_vu"></td>
                                <td><input type="text" style="height: 100%;padding-left: 5px;width:100%" class="form-control" ng-model="arraylienhe[$index].phong_ban"></td>
                                <td>
                                    <input type="text" style="height: 100%;padding-left: 5px;width:100%" class="form-control" datepicker ng-model="arraylienhe[$index].ngay_sinh">
                                </td>
                                <td>
                                    <input type="text" style="height: 100%;padding-left: 5px;width:100%" class="form-control" ng-model="arraylienhe[$index].gioi_tinh">
                                </td>
                                <td>
                                    <input placeholder="Số điện thoại 1(*)" type="tel" class="form-control" ng-model="arraylienhe[$index].so_dien_thoai1">
                                    <input placeholder="Số điện thoại 2" type="tel" class="form-control" ng-model="arraylienhe[$index].so_dien_thoai2">
                                </td>
                                <td>
                                    <input placeholder="Email cá nhân" type="text" class="form-control" ng-model="arraylienhe[$index].email_ca_nhan">
                                    <input placeholder="Email công ty" type="text" class="form-control" ng-model="arraylienhe[$index].email_cong_ty">
                                </td>
                                <td><input type="text" class="form-control" ng-model="arraylienhe[$index].skype"></td>
                                <td><input type="text" style="height: 100%;padding-left: 5px;width:100%" class="form-control" ng-model="arraylienhe[$index].facebook"></td>
                                <td>
                                    <select ng-model="arraylienhe[$index].pur_phu_trach" class="form-control input-md">
                                        <option ng-repeat="abc in list_nhanvienmua" value="{{abc.USERNAME}}">{{abc.HO_VA_TEN}}</option>
                                    </select>
                                </td>
                            </tr>
                        </table>
                        <button type="button" class="fa fa-plus button_addrow" ng-click="AddNew1()"><span style="font-size: 15px; font-weight: bold; padding-left:5px; padding-top:5px;">Thêm dòng</span></button>
                        <script>
                                CKEDITOR.replace('addghichu', {
                                        height: 100
                                    });
                                CKEDITOR.replace('danh_gia', {
                                        height: 100
                                    });
                        </script>
                    </form>
                </div>
                <div class="modal-footer" style="text-align: center;margin-top: 20px;">
                    <button id="save" name="save" class="btn fa fa-save" data-dismiss="modal" ng-click="createnew()">Lưu</button>
                    <button id="exit" name="exit" class="btn fa fa-sign-out" data-dismiss="modal">Thoát</button>
                </div>
            </div>
        </div>
    </div>
    <!--end modal-->
    <!-- Modal Update nha cung cap-->
    <div class="modal fade" id="myEdit" role="dialog" tabindex="-1">
        <div class="modal-dialog modal-lg" style="width: 90%">
            <div class="modal-content" style="height: 100%">
                <div class="mhead">
                    <h2>SỬA THÔNG TIN NHÀ CUNG CẤP</h2>
                </div>
                <div class="modal-body">
                    <form class="form-horizontal" action="/NhaCungCap/Index" method="post">
                        <p style="text-shadow: 1px 1px 5px #91DDFD; font-style:italic; font-size: 20px; color: #64B5F6; margin-left:10px; margin-top:5px; font-weight: bold">SỬA THÔNG TIN CHUNG</p>
                        <div class="line-bottom"></div>
                        <table border="1" style="width: 100%;">
                            <tr>
                                <th class="text_edit">Mã nhà cung cấp</th>
                                <td colspan="5"><input type="text" class="form-control" disabled="disabled" id="addmakhachhang" ng-model="ncc.MA_NHA_CUNG_CAP"></td>
                                <td colspan="2" rowspan="5">
                                    <p style="text-shadow: 1px 1px 5px #91DDFD; font-style:italic; font-size: 20px; color: #64B5F6; margin-left:10px; margin-top:5px;margin-bottom:15px; font-weight: bold">Ảnh đại diện</p>
                                    <img id="edit_img" style="width: 100px; height: 100px;" src="/Content/self-writing/Images/default_avatar_male.jpg" />
                                    @Html.AntiForgeryToken()
                                    <center><input type='file' name="files" id="imgEdit" style="margin-top: 10px;" /></center>
                                </td>
                            </tr>
                            <tr>
                                <th colspan="1" class="text_edit">Tên nhà cung cấp</th>
                                <td colspan="5"><input type="text" class="form-control" id="addtencongty" ng-model="ncc.TEN_NHA_CUNG_CAP"></td>
                            </tr>
                            <tr>
                                <th colspan="1" class="text_edit">Văn phòng giao dịch</th>
                                <td colspan="5"><input type="text" class="form-control" id="addvpgiaodich" ng-model="ncc.VAN_PHONG_GIAO_DICH"></td>
                            </tr>
                            <tr>
                                <th colspan="1" class="text_edit">Địa chỉ xuất hóa đơn</th>
                                <td colspan="5"><input type="text" class="form-control" id="adddiachixuathoadon" ng-model="ncc.DIA_CHI_XUAT_HOA_DON"></td>
                            </tr>
                            <tr>
                                <th class="text_edit">Phân loại NCC</th>
                                <td colspan="5">
                                    <label class="radio-inline" ng-repeat="phanloai in list_phanloai">
                                        <input type="radio" name="optradio" ng-value="phanloai.MA_LOAI_NCC" ng-model="ncc.MA_LOAI_NCC">{{phanloai.TEN_LOAI_NCC}}
                                    </label>
                                </td>
                            </tr>
                            <tr>
                                <th class="text_edit">Mã số thuế</></th>
                                <td><input type="text" class="form-control" id="addmasothue" ng-model="ncc.MST"></td>
                                <th class="text_edit">Website</th>
                                <td><input type="text" class="form-control" id="addwebsite" ng-model="ncc.WEBSITE"></td>
                                <th class="text_edit">Số máy bàn</th>
                                <td><input type="tel" class="form-control" id="addsomayban" ng-model="ncc.SDT"></td>
                                <th class="text_edit">Fax</th>
                                <td><input type="text" class="form-control" id="addfax" ng-model="ncc.FAX"></td>
                            </tr>

                            <tr>
                                <th class="text_edit">Điều khoản thanh toán</th>
                                <td>
                                    <select class="form-control" ng-model="ncc.DIEU_KHOAN_THANH_TOAN" id="adddieukhoanthanhtoan"
                                            data-ng-options="item as item for item in dieukhoantt"></select>
                                </td>
                                <th class="text_edit">Số ngày được nợ</th>
                                <td><input type="number" class="form-control" id="addsongayduocno" ng-model="ncc.SO_NGAY_DUOC_NO"></td>
                                <th class="text_edit">Số nợ tối đa</th>
                                <td><input type="number" class="form-control" id="addsonotoida" ng-model="ncc.SO_NO_TOI_DA"></td>
                                <th class="text_edit">Email</th>
                                <td><input type="text" class="form-control" id="addemail" ng-model="ncc.EMAIL"></td>
                            </tr>
                            <tr>
                                <th class="text_edit">Ghi chú</th>
                                <td colspan='3'>
                                    <textarea id="editghichu" name="editghichu" placeholder="Ghi chú" class="form-control input-md" ng-model="ncc.GHI_CHU"></textarea>
                                </td>
                                <th class="text_edit">Đánh giá</th>
                                <td colspan='3'>
                                    <textarea id="editdanh_gia" name="editdanh_gia" placeholder="Ghi chú" class="form-control input-md" ng-model="ncc.DANH_GIA"></textarea>
                                </td>
                            </tr>
                        </table>
                        <script>
                            CKEDITOR.replace('editghichu', {
                                height: 100
                            });
                            CKEDITOR.replace('editdanh_gia', {
                                height: 100
                            });
                        </script>
                    </form>
                </div>
                <div class="modal-footer" style="text-align: center;margin-top: 20px;">
                    <button id="save" name="save" class="btn fa fa-save" data-dismiss="modal" ng-click="save(ncc.MA_NHA_CUNG_CAP)">Lưu</button>
                    <button id="exit" name="exit" class="btn fa fa-sign-in" data-dismiss="modal">Thoát</button>
                </div>
            </div>
        </div>
    </div>
    <br />
    <!--end modal-->
    <!-- Modal chi tiet lien he -->
    <div class="modal fade" id="chitietlienhe" role="dialog" tabindex="-1">
        <div class="modal-dialog modal-lg" style="width:80%;">
            <div class="modal-content" style="height: 450px;">
                <div class="modal-body">
                    <div class="col-md-12" style="box-shadow: 3px 3px 3px #ddd">
                        <div class="col-md-6">
                            <table class="table noborder">
                                <tr>
                                    <td>Tên nhà cung cấp</td>
                                    <td>{{lienhe.TEN_NHA_CUNG_CAP}}</td>
                                </tr>
                                <tr>
                                    <td>Người liên hệ</td>
                                    <td>{{lienhe.NGUOI_LIEN_HE}}</td>
                                </tr>
                                <tr>
                                    <td>Chức vụ</td>
                                    <td>{{lienhe.CHUC_VU}}</td>
                                </tr>
                                <tr>
                                    <td>Phòng ban</td>
                                    <td>{{lienhe.PHONG_BAN}}</td>
                                </tr>
                                <tr>
                                    <td>Ngày sinh:</td>
                                    <td>{{lienhe.NGAY_SINH}}</td>
                                </tr>

                                <tr>
                                    <td>Email cá nhân:</td>
                                    <td>{{lienhe.EMAIL_CA_NHAN}}</td>
                                </tr>
                                <tr>
                                    <td>Email công ty:</td>
                                    <td>{{lienhe.EMAIL_CONG_TY}}</td>
                                </tr>
                            </table>
                        </div>
                        <div class="col-md-6">
                            <table class="table noborder">
                                <tr>
                                    <td>Giới tính:</td>
                                    <td>{{lienhe.GIOI_TINH}}</td>
                                </tr>
                                <tr>
                                    <td>Skype</td>
                                    <td>{{lienhe.SKYPE}}</td>
